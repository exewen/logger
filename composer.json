{
    "name": "exewen/logger",
    "description": "A logger component for Exewen.",
    "authors": [
        {
            "name": "exewen",
            "email": "exeweb@163.com"
        }
    ],
    "license": "MIT",
    "type": "library",
    "keywords": [
        "php",
        "exewen",
        "logger"
    ],
    "require": {
        "php": ">=7.3",
        "exewen/di": "^1",
        "monolog/monolog": "^1.24 || ^2.0 || ^3.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7 | ^6.5 | ^7.5 | ^8.4 | ^9.3"
    },
    "autoload": {
        "psr-4": {
            "Exewen\\Logger\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ExewenTest\\Logger\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "config": {
        "sort-packages": true
    },
    "extra": {
        "exewen": {
            "config": "Exewen\\Logger\\ConfigRegister"
        }
    }
}
