{
    "name": "exewen/logger",
    "description": "A logger component for Exewen.",
    "authors": [
        {
            "name": "exewen",
            "email": "exeweb@163.com"
        }
    ],
    "license": "MIT",
    "type": "library",
    "keywords": [
        "php",
        "exewen",
        "logger"
    ],
    "require": {
        "php": ">=7.4",
        "exewen/di": "*",
        "monolog/monolog": "^1.24|^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7 | ^6.5 | ^7.5 | ^8.4 | ^9.3"
    },
    "autoload": {
        "psr-4": {
            "Exewen\\Logger\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ExewenTest\\Logger\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        },
        "exewen": {
            "config": "Exewen\\Logger\\ConfigRegister"
        }
    }
}
